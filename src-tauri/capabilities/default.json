{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "updater:default",
    "dialog:default",
    "fs:allow-app-write",
    "fs:allow-app-read",
    "shell:default",
    "fs:read-all",
    "fs:write-all"
  ]
}
